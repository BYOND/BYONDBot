{
  "name": "byondbot",
  "version": "1.0.0",
  "description": "A bot for the BYOND discord server.",
  "main": "index.js",
  "engines": {
    "node": "10.11.0"
  },
  "scripts": {
    "start": "ts-node src/index",
    "start:dev": "nodemon --exec \"ts-node \"-- \"src/index.ts\"",
    "test:lint": "tslint --project .",
