    "test": "npm run test:lint && npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BYOND/DiscordBot.git"
  },
  "author": "Kyle Kemp <kyle@seiyria.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BYOND/DiscordBot/issues"
  },
  "homepage": "https://github.com/BYOND/DiscordBot#readme",
  "dependencies": {
    "axios": "0.21.1",
    "cheerio": "1.0.0-rc.3",
    "discord.js": "^12.5.3",
    "dotenv": "^8.2.0",
    "fuzzyset-obj": "1.0.0",
    "turndown": "6.0.0",
    "typeorm": "^0.2.34",
    "typescript-ioc": "^3.2.2"
  },
  "devDependencies": {
    "ava": "^3.11.1",
    "ava-ts": "^0.25.2",
    "nodemon": "2.0.4",
    "ts-node": "^10.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
